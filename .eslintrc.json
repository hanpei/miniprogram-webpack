{
    "globals": {
        "Page": true,
        "App": true,
        "wx": true,
        "Component": true,
        "getApp": true,
        "getCurrentPages": true
    },
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
 
  
    "rules": {
    "semi": [2, "always"],
    "comma-dangle": ["error", "always-multiline"],
    "space-before-function-paren": "off"
    }
}
